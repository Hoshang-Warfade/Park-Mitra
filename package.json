{
  "name": "parkmitra-backend",
  "version": "1.0.0",
  "description": "ParkMitra parking management backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd frontend && npm start",
    "test": "echo \"Backend tests not yet implemented\" && exit 0",
    "build": "cd frontend && npm run build",
    "init-db": "node backend/database/init-db.js",
    "seed-db": "node backend/database/seed-db.js",
    "setup-db": "npm run init-db && npm run seed-db",
    "reset-db": "del /f parkmitra.db 2>nul & npm run init-db & npm run seed-db",
    "test-db": "node backend/database/test-db.js",
    "view-db": "node backend/database/view-db.js",
    "sync-slots": "node backend/database/sync-all-slots.js"
  },
  "keywords": [
    "parking",
    "management",
    "booking"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "qrcode": "^1.5.3",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
